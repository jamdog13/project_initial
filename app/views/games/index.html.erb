<% provide(:title,"Game List")%>
<% @games.each do |game| %>
		<%if !game.name.nil?%>
			<% targ = games_path + "/" + game.id.to_s %>
			<%=link_to game.name, targ%>  Times played: <%=game.times_played%> High Score: 
			<% @highscore=Score.where(game_id:game.id).order(value: :desc).first %>
			<%= render @highscore %>
		<%end%>
<%end%>